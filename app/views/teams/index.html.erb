<h1>Listing teams</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>City</th>
    <th>Country iso</th>
    <th>Site</th>
    <th>Facebook</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= team.name %></td>
    <td><%= team.address %></td>
    <td><%= team.city_id %></td>
    <td><%= team.country_iso %></td>
    <td><%= team.site %></td>
    <td><%= team.facebook %></td>
    <td><%= team.location_id %></td>
    <td><%= link_to 'Show', team %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td><%= link_to 'Destroy', team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Team', new_team_path %>
