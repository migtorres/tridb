<%- model_class = ImportResultsFromUrl -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:url) %>:</strong></dt>
  <dd><%= @import_results_from_url.url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:import_type) %>:</strong></dt>
  <dd><%= @import_results_from_url.import_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:race_type_id) %>:</strong></dt>
  <dd><%= @import_results_from_url.race_type_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              import_results_from_urls_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_import_results_from_url_path(@import_results_from_url), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              import_results_from_url_path(@import_results_from_url),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
