<%- model_class = ImportResultsFromUrl -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:url) %></th>
      <th><%= model_class.human_attribute_name(:type) %></th>
      <th><%= model_class.human_attribute_name(:quantity) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @import_results_from_urls.each do |import_results_from_url| %>
      <tr>
        <td><%= link_to import_results_from_url.id, import_results_from_url_path(import_results_from_url) %></td>
        <td><%= import_results_from_url.url %></td>
        <td><%= import_results_from_url.type %></td>
        <td><%= import_results_from_url.quantity %></td>
        <td><%=l import_results_from_url.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_import_results_from_url_path(import_results_from_url), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      import_results_from_url_path(import_results_from_url),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_import_results_from_url_path,
            :class => 'btn btn-primary' %>
